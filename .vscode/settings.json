{
    "files.associations": {
        "ostream": "cpp",
        "iostream": "cpp",
        "vector": "cpp"
    },
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
    "cpp": "cmd /c \"cd $dir && g++ $fileName -o $fileNameWithoutExt.exe && $fileNameWithoutExt.exe\"",
    "c": "cmd /c \"cd $dir && gcc $fileName -o $fileNameWithoutExt.exe && $fileNameWithoutExt.exe\""
    },
    "code-runner.clearPreviousOutput": true,
    "terminal.integrated.defaultEncoding": "shiftjis"
}